{
    "tracker_default": {
        "type": "container",
        "background": "#000000",
        "content": {
            "type": "dock",
            "dropshadow": true,
            "content": [
                {
                    "type": "dock",
                    "dock": "bottom",
                    "content": [
                        {
                            "type": "array",
                            "orientation": "vertical",
                            "margin": "0,0",
                            "dock": "left",
                            "content": [
                                {
                                    "type": "group",
                                    "header": "Important Items",
                                    "dock": "left",
                                    "margin": "0,0,3,0",
                                    "content": {
                                        "type": "layout",
                                        "h_alignment": "center",
                                        "v_alignment": "center",
                                        "key": "wincondition_grid"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "array",
                            "orientation": "vertical",
                            "margin": "0,0",
                            "dock": "left",
                            "content": [
                                {
                                    "type": "group",
                                    "header": "Goal Condition",
                                    "dock": "left",
                                    "margin": "0,0,3,0",
                                    "content": {
                                        "type": "layout",
                                        "h_alignment": "center",
                                        "v_alignment": "center",
                                        "key": "goalsettings_grid"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "array",
                            "orientation": "vertical",
                            "margin": "0,0",
                            "dock": "left",
                            "content": [
                                {
                                    "type": "group",
                                    "header": "Settings",
                                    "dock": "left",
                                    "margin": "0,0,3,0",
                                    "content": {
                                        "type": "layout",
                                        "h_alignment": "center",
                                        "v_alignment": "center",
                                        "key": "settings_grid"
                                    }
                                }
                            ]
                        }
                        

                    ]
                },
                {
                    "type": "dock",
                    "content": {
                        "type": "map",
                        "maps": [ "menu" ]
                    }
                }
            ]
        }
    }
}